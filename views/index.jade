    script(type="text/javascript").
      var myScroll; 
      function loaded() { 
        myScroll = myScroll = new IScroll('#wrapper', { eventPassthrough: true,
         scrollX: true, 
         scrollY: false, 
         preventDefault: false, 
         mouseWheel: false, 
         scrollbars: true,
         fadeScrollbars: false, });
        }      
  body#page-top(onload="loaded()", onselectstart="return false;", ondragstart="return false;")
    nav#mainNav.navbar.navbar-default.navbar-fixed-top
      .container-fluid
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle.collapsed(type="button", data-toggle="collapse", data-target="#bs-example-navbar-collapse-1")
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand.page-scroll(href="#page-top")#typeOut Yacine Brahimi 
        // Collect the nav links, forms, and other content for toggling
        #bs-example-navbar-collapse-1.collapse.navbar-collapse
      // /.container-fluid
    header#header
      .header-content
        .header-content-inner
          h1 Hi! 
          hr
          p My name is Yacine Brahimi. I am an aspiring software engineer who is currently enrolled in Carleton University's computer science program. Currently, I am looking to broaden my knowledge and experience by working in the field either as a freelance developer or an intern.
          a.btn.btn-primary.btn-xl.page-scroll.wow.swing(href="#about", data-wow-delay="3s", data-wow-iteration="2", data-wow-duration="2s") Find Out More     
    section#about
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading At your service 
            hr.primary
            p.text-muted
      .container
        .row
          .col-lg-3.col-md-6.text-center
            .span4.collapse-group
              .service-box
                a.btn.btn-primary(type="button", data-toggle="collapse", data-target="#demo")
                  #graduation-cap.i.fa.fa-4x.fa-graduation-cap.wow.bounceIn.text-default
                h3 Carleton University
                #demo.p.collapse
                  div(ng-init="info=['Software Engineering stream', 'Currently in third year standing']") 
                  ul(ng-repeat="x in info").list-unstyled
                    p {{x}}     
                p.text-muted Bachelor of Computer Science Honours
          .col-lg-3.col-md-6.text-center
            .service-box
              .btn.btn-primary.disabled(type="button", data-toggle="collapse", data-target="#languages")
                i.fa.fa-4x.fa-language.wow.bounceIn.text-default(data-wow-delay=".2s")
                  #map-canvas.div
              h3 Languages 
              p.text-muted Fluent in both English and Arabic, and can speak un peu de Francais 
          .col-lg-3.col-md-6.text-center
            .service-box
              .btn.btn-primary(type="button", data-toggle="collapse", data-target="#programming")
                i.fa.fa-4x.fa-keyboard-o.wow.bounceIn.text-default(data-wow-delay=".5s")
              h3 Programming
              #programming.p.collapse
                div(ng-init="programming=['C', 'C++', 'Java', 'Javascript', 'Typescript', 'React.js', 'Node.js', 'HTML/CSS/bootstrap/Jade', 'Python', 'GNU/Linux', 'Mac OS', 'Windows']")
                ul(ng-repeat="x in programming").list-unstyled
                  p {{ x }}
              p.text-muted Proficient in several programming languages.
          .col-lg-3.col-md-6.text-center
            .service-box
              .btn.btn-primary(type="button", data-toggle="collapse", data-target="#hobbies")
                i.fa.fa-4x.fa-user.wow.bounceIn.text-default(data-wow-delay=".7s")
              h3 Hobbies and interests
              #hobbies.p.collapse
                div(ng-init="hobbies=['Programming', 'Guitar', 'Travelling', 'Longboarding', 'Weight lifting and fitness', 'Cooking', 'Literature', 'Cats']")
                ul(ng-repeat="x in hobbies").list-unstyled
                  p {{ x }}
              p.text-muted Things I do for the sake of doing
    section#instagram.no-padding
      aside.bg-dark
        .container.text-center
          .call-to-action
            h2 Find me on Instagram!
            hr
            a.btn.btn-default.btn-xl(type="button", data-target="#socialMedia", href="https://instagram.com/yacinemtbee/", target="_blank")
              i.fa.fa-4x.fa-instagram.text-default.wow.bounceIn(data-wow-delay="0.2s", data-wow-duration="2s")
        aside.container-fluid(style="padding-left:0px; padding-right:0px")
          #wrapper
            #scroller
              #insta

    section#contact
      .container
        .row
          .col-lg-8.col-lg-offset-2.text-center
            h2.section-heading Get in touch!
            hr.primary
            p.text-muted
              | You can check out my github, send me an email or call me!
        .row 
          .col-md-4.text-center
            i.fa.fa-phone.fa-3x.wow.bounceIn
            p.text-muted 613-400-7379
          .col-md-4.text-center
            i.fa.fa-github.fa-3x.wow.bounceIn
            p
              a(href="http://github.com/yacinemtb") yacineMTB
